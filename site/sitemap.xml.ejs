<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>http://blog.richardhinkamp.nl/</loc>
    </url>
    <% public._contents.forEach(function( page ) { %>
        <% if ( page.match(/\.html$/) && page != 'index.html'&& page != '404.html' ) { %>
        <url>
            <loc>http://blog.richardhinkamp.nl/<%= page %></loc>
        </url>
        <% } %>
    <% }) %>
    <% public.blogs._contents.forEach(function( blog ) { %>
        <% if ( blog.match(/\.html$/) ) { %>
            <url>
                <loc>http://blog.richardhinkamp.nl/blogs/<%= blog %></loc>
            </url>
        <% } %>
    <% }) %>
</urlset>
